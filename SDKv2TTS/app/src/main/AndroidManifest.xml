<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bfr.sdkv2_tts">

    <uses-permission android:name="com.bfr.buddy.resource.SPEECH" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SDKv2TTS"
        android:name="com.bfr.sdkv2_tts.AppActivity"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/BuddyTheme"
        android:supportsRtl="true">

        <activity
            android:name="com.bfr.sdkv2_tts.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>